{
  "name": "ex-cleanse",
  "displayName": "ex-cleanse",
  "description": "A utility that cleans up logs in your Elixir application.",
  "version": "1.0.0",
  "publisher": "glamboyosa",
  "engines": {
    "vscode": "^1.67.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:ex-cleanse.Cleanse",
    "onCommand:ex-cleanse.addLogger"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "languages": [
      {
        "id": "Elixir",
        "extensions": [
          ".ex",
          ".exs"
        ],
        "aliases": [
          "Elixir",
          "ex"
        ]
      }
    ],
    "keybindings": [
      {
        "command": "ex-cleanse.Cleanse",
        "key": "ctrl+shift+a",
        "mac": "cmd+shift+a",
        "when": "editorTextFocus"
      }
    ],
    "commands": [
      {
        "command": "ex-cleanse.Cleanse",
        "title": "Cleanse"
      },
      {
        "command": "ex-cleanse.addLogger",
        "title": "Add Logger"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/vscode": "^1.67.0",
    "@types/glob": "^7.2.0",
    "@types/mocha": "^9.1.1",
    "@types/node": "16.x",
    "@typescript-eslint/eslint-plugin": "^5.27.0",
    "@typescript-eslint/parser": "^5.27.0",
    "eslint": "^8.16.0",
    "glob": "^8.0.3",
    "mocha": "^10.0.0",
    "typescript": "^4.7.2",
    "@vscode/test-electron": "^2.1.3"
  }
}
